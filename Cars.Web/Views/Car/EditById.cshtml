
@model CreateCarViewModel
@{
    ViewData["Title"] = "Edit";
}

<form method="post">

    <div
    <label asp-for="Price"></label>
  <input asp-for="Price" />
    </div>

    <div
    <label asp-for="Category">Category</label>
    <select asp-for="Category">
        <option value="Coupe">Sport Coupe</option>
        <option value="Limousine">Limousine</option>
        <option value="Cabrio">Cabrio</option>
        <option value="Van">Van</option>
        <option value="Jeep">Jeep</option>

    </select>
    </div>
    <div
    <label asp-for="HorsePower"></label>
  <input asp-for="HorsePower" />
    </div>
    <div
    <label asp-for="TypeOfEngine">Type of Engine</label>
    <select asp-for="TypeOfEngine">
        <option value="Benzin">Benzin</option>
        <option value="Diesel">Diesel</option>
        <option value="Hybrid">Hybrid</option>

    </select>
    </div>
    <div
    <label asp-for="TypeOfGearbox">Type Of Gearbox</label>
    <select asp-for="TypeOfGearbox">
        <option value="Automatic">Automatic</option>
        <option value="Manual">Manual</option>


    </select>
    </div>
    <div
    <label asp-for="YearOfBuilding"></label>
  <input asp-for="YearOfBuilding" />
    </div>


    <label asp-for="Description"></label>
<textarea asp-for="Description"> </textarea>

    <input type="submit" value="sub" />

</form>

